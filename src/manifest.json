{
  "manifest_version": 2,
  "name": "quiz crawler",
  "version": "0.1.0" ,
  "description": "A chrome extension to extract quiz for some specific teaching platforms.",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "quiz crawler",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
